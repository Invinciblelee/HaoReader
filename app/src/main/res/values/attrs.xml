<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer" />
        <attr name="stroke_width" format="dimension" />
        <attr name="color_tick" format="color" />
        <attr name="color_checked" format="color" />
        <attr name="color_unchecked" format="color" />
        <attr name="color_unchecked_stroke" format="color" />
    </declare-styleable>

    <declare-styleable name="NumberPickerPreference">
        <attr name="MinValue" format="integer" />
        <attr name="MaxValue" format="integer" />
        <attr name="android:summary" />
    </declare-styleable>

    <attr name="appInsetForeground" format="reference" />
    <attr name="appConsumeInsets" format="boolean" />

    <declare-styleable name="ScrimInsetsRelativeLayout">
        <attr name="appInsetForeground" />
        <attr name="appConsumeInsets" />
        <attr name="fitTop" format="boolean" />
        <attr name="fitBottom" format="boolean" />
        <attr name="fitLeft" format="boolean" />
        <attr name="fitRight" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ScaleDrawableTextView">
        <attr name="drawableScale" format="reference|float" />
        <attr name="drawableColor" format="reference" />
        <attr name="drawableSize" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <attr name="strokeWidth" format="dimension|reference" />
        <attr name="strokeColor" format="color|reference" />
        <attr name="maxProgress" format="float|reference" />
        <attr name="progress" format="float|reference" />
    </declare-styleable>

    <declare-styleable name="TextInputSpinner">
        <attr name="android:entries" />
    </declare-styleable>

    <declare-styleable name="NumberButton">
        <attr name="themeColor" format="color|reference" />
        <attr name="android:background" />
    </declare-styleable>

    <declare-styleable name="AbsRoundImageView">
        <attr name="borderWidth" format="dimension|reference" />
        <attr name="borderColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="RotateLoading">
        <attr name="loadingWidth" format="dimension|reference" />
        <attr name="loadingColor" format="color|reference" />
        <attr name="shadowPosition" format="integer|reference" />
        <attr name="loadingSpeed" format="integer|reference" />
    </declare-styleable>

    <declare-styleable name="FastScroller">
        <attr name="hideScrollbar" format="boolean" />
        <attr name="showBubble" format="boolean" />
        <attr name="showTrack" format="boolean" />
        <attr name="bubbleColor" format="color" />
        <attr name="bubbleTextColor" format="color" />
        <attr name="handleColor" format="color" />
        <attr name="trackColor" format="color" />
        <attr name="bubbleTextSize" format="dimension" />
        <attr name="bubbleSize" format="enum">
            <enum name="normal" value="0" />
            <enum name="small" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LabelView">
        <attr name="labelColor" format="color|reference" />
        <attr name="labelSinkWidth" format="dimension|reference" />
    </declare-styleable>

</resources>